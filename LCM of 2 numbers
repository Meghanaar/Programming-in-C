#include<stdio.h>
#include<math.h>
int lcm(int a,int b);
int main()
{
	int x=0,y=0,result=0;
	printf("Enter any two number ");
	scanf("%d %d",&x,&y);
	result=lcm(x,y);
	printf("The LCM of the given number %d and %d is %d",x,y,result);	
	return 0;
}

int lcm(int num,int z)
{
	int a;
	for(a=1;a!=0;a++)
	{
		if(a%num==0 && a%z==0)
			return a;	
	}
}
