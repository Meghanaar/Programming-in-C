#include<stdio.h>

main()
{
	int food;
	printf("Enter any number between 1 to 7:");
	scanf("%d",&food);
	switch(food)
	{
		case 1:
			printf("Food item:Masala Dose\nPrice:120");
			break;
		case 2:
			printf("Food item:French Fries\nPrice:99");
			break;
		case 3:
			printf("Food item:Idly Vada\nPrice:100");
			break;
		case 4:
			printf("Food item:Pizza\nPrice:179");
			break;
		case 5:
			printf("Food item:Biryani\nPrice:249");
			break;
		case 6:
			printf("Food item:Pasta\nPrice:179");
			break;
		case 7:
			printf("Food item:Sandwich\nPrice:149");
			break;
		default:
			printf("INVALID, please choose a number between 1 to 7");
			break;
	}
	return 0;
}
